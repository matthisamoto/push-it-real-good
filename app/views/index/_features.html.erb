<div class="lower-content clearfix">
	
  <div class="top-ten">
	<h1>Top Ten Pushes</h1>
    <% @top_ten.each do |top_item| %>
	<div class="top-item">
	  <div class="button-image"><%= image_tag "/images/#{top_item.button_url}.png", :height => 40, :width => 80 %></div>
	  <div class="page-info">
	    <div class="user-page-url"><%= link_to top_item.title_url, "/page/#{top_item.title_url}" %></div> <div class="user-page-tagline"><%= top_item.tagline %></div>
	  </div>
	  <div class="user-links">
	    <%= link_to "", "/page/#{top_item.title_url}", :class => "visit-btn" %>
	  </div>
    </div>
    <% end %>
  </div>

  <div class="featured-button">
    		
  </div>
	
</div>